    "markdown-it-testgen": "^0.1.6",
    "mocha": "10.4.0",
    "mocha-junit-reporter": "2.2.1",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "lib",
      "test"
    ]
